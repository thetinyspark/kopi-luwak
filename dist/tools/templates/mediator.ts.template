import { IMediator, Facade, RegisterMediator } from "@thetinyspark/kopi-luwak";
import { <%= token %> } from "../config/AppConsts";

@RegisterMediator({token: <%= token %> })
export class <%= className %> implements IMediator {
    private facade: Facade;

    constructor() {}

    getFacade(): Facade {
        return this.facade;
    }

    setFacade(facade: Facade): void {
        this.facade = facade;
    }
}